=content_for :h1 do
  %span.type=@content.media_type.capitalize
  =@content.asset.name
  #action
    %span.action
      =link_to 'show', "/fp/presentations/"+@content.id.to_s
      =#link_to 'delete', asset_path(@content.asset), :method => :delete

#sidebar
  =render :partial => "shared/classification", :locals => {:asset => @content.asset}
  =render :partial => "fassets-presentations/presentations/frames", :locals => {:presentation =>  @content, :options => nil}
  
#main
  =#render :partial => "shared/asset_form", :locals => {:model => @content, :asset => @content.asset, :submit => "Save",:options => nil}
